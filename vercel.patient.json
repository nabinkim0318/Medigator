{
  "version": 2,
  "name": "bbb-patient",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.railway.app/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-backend-url.railway.app",
    "NEXT_PUBLIC_APP_TYPE": "patient"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_TYPE": "patient"
    }
  }
}
